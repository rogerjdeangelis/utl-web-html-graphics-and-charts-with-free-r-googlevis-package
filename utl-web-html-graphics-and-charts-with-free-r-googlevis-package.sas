%utlfkil(d:/htm/googorgcht.html);
%utl_submit_r64('
library(haven);
library(googleVis);
Regions<-read_sas("d:/sd1/havOrg.sas7bdat");
Regions[1,2]<-NA;
Regions$Parent<-as.factor(Regions$Parent);
Regions$Region<-as.factor(Regions$Region);
Regions$Val<-as.integer(Regions$Val);
Regions$Fac<-as.integer(Regions$Fac);
Regions<-data.frame(Regions);
Regions;
str(Regions);
Org <- gvisOrgChart(Regions,
 options=list(width=600, height=250,
 size="large", allowCollapse=TRUE));
print(Org, file="d:/htm/googorgcht.html");
');

